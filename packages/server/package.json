  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development NODE_PATH=./src nodemon --exec ts-node ./src/index.ts",
    "preview": "cross-env NODE_ENV=production NODE_PATH=./src ts-node ./dist/index.js",
    "build": "tsc",
    "postgres:up": "docker-compose up -d",
    "postgres:down": "docker-compose down",
    "postgres:clear": "docker-compose down -v --rmi all",
    "prisma:db:pull": "prisma db pull",
    "prisma:db:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset -f"
  },
